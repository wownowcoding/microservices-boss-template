{
  "remoteName": "MicroservicesBossMarketing",
  "name": "microservices-boss-marketing",
  "version": "2.8.0",
  "description": "WOWNOW App Boss",
  "author": "hbf",
  "private": true,
  "build": true,
  "egg": {
    "declarations": true
  },
  "scripts": {
    "serve": "node --max_old_space_size=4096 node_modules/.bin/vue-cli-service serve",
    "build:cli": "node --max_old_space_size=4096 node_modules/.bin/vue-cli-service build",
    "dev": "node --max_old_space_size=4096 & npm run serve",
    "dev-part": "node --max_old_space_size=4096 & buildPart=true npm run serve",
    "init-part": "node ./scripts/ineedCompileDirectoryPlugin/index.js",
    "start": "npm run serve",
    "build": "npm run build:cli --report",
    "build_sit_local": "npm run build:cli",
    "build_prod": "npm run build:cli",
    "build_uat": "npm run build:cli",
    "build_uat_sub": "npm run build:cli",
    "build_sit": "npm run build:cli",
    "build_pre": "npm run build:cli",
    "lang_export": "node src/language/export",
    "check-branch": "node --max-old-space-size=8000 ./scripts/build/commit/commit-check.js",
    "analyzer": "cross-env use_analyzer=true npm run build_sit",
    "clear-git": "node ./scripts/submit-versions.js",
    "导出webpack配置": "npx vue-cli-service inspect --mode production >> webpack.config.production.js",
    "i18nv2": "cross-env fileName=script_auto_v2 key=scriptv2 node i18n-script-v2",
    "i18n": "cross-env fileName=wm_3_0_5 key=wm_3_0_5 node i18n-script",
    "test-apollo": "node --max-old-space-size=8000 preload.js --src='http://service-apollo-config-server-sit.lifekh-tool-sit.svc.cluster.local:8080'",
    "increase": "cross-env LIMIT=10240 increase-memory-limit"
  },
  "dependencies": {
    "@elephant-wownow/kubernetes-pod": "^1.2.2",
    "axios": "0.21.1",
    "crypto-js": "4.0.0",
    "flat": "^5.0.2",
    "glob": "7.1.6",
    "google-maps-api-loader": "1.1.1",
    "http-proxy": "1.18.1",
    "humps": "2.0.1",
    "ip": "1.1.5",
    "money-module": "1.0.6",
    "node-uuid": "1.4.8",
    "node-xlsx": "0.15.0",
    "xml2js": "0.4.22",
    "xss": "1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.10.4",
    "@babel/plugin-proposal-optional-chaining": "7.10.4",
    "@babel/plugin-transform-spread": "7.10.4",
    "@ckeditor/ckeditor5-build-decoupled-document": "31.1.0",
    "@ckeditor/ckeditor5-font": "31.1.0",
    "@ckeditor/ckeditor5-vue2": "1.0.5",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "axios-mock-adapter": "1.15.0",
    "babel-plugin-component": "1.1.1",
    "babel-plugin-import": "1.13.0",
    "babel-polyfill": "6.26.0",
    "caniuse-lite": "1.0.30001378",
    "clipboard": "2.0.8",
    "core-js": "^3.25.2",
    "cross-env": "^6.0.3",
    "directory-named-webpack-plugin": "4.0.1",
    "element-ui": "2.13.2",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "file-saver": "2.0.5",
    "hard-source-webpack-plugin": "0.13.1",
    "increase-memory-limit": "^1.0.7",
    "iview": "3.4.0",
    "js-cookie": "2.1.3",
    "jsencrypt": "3.0.0-rc.1",
    "less": "3.11.3",
    "less-loader": "6.2.0",
    "lodash": "4.17.11",
    "md5": "2.2.1",
    "moment": "2.20.1",
    "moment-timezone": "0.5.26",
    "opn": "5.1.0",
    "pre-commit": "1.2.2",
    "progress": "2.0.3",
    "qrcode": "1.2.2",
    "stylus": "0.54.7",
    "stylus-loader": "3.0.2",
    "v-viewer": "1.3.2",
    "vue": "2.6.14",
    "vue-cropper": "0.5.6",
    "vue-i18n": "7.4.1",
    "vue-loader": "13.3.0",
    "vue-router": "3.0.1",
    "vue-style-loader": "3.0.1",
    "vue-template-compiler": "2.6.14",
    "vuex": "2.5.0",
    "webpack-bundle-analyzer": "3.7.0",
    "xlsx": "0.14.5"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "pre-commit": [
    "check-branch"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ]
}
